# Starting meta values
StartingValues:
  Honey: 240
  Coins: 60

TournamentLeaderboardMaxScoreDifferentialMultiplier: 2

# How many tokens it takes to buy a bear
BearTokenCosts:
  Common: 10
  Rare: 20
  Epic: 100
  Legendary: 200

BoobyPrizeLootTables:
  Campaign: CampaignBoobyPrize
  School: DailyAndSchoolsBoobyPrize
  Daily: DailyAndSchoolsBoobyPrize

CampaignScoreTargetFloorSettings:
  DifficultyAdjustmentMultiplier: 0.74
  UseNBestBears: 3
  UseNthBestBear: 0
  OnlyUseAwakeBears: true
  NumGamesForMedianSkillRating: 11

BigBearBonusMultiplier: 4

# Bear select power bar settings
ImpossibleThresholdAverageLetterScore: 6

# Modes are unlocked after WINNING these chapter events.
# Chapter numbers and mission numbers in this section are 1-indexed.
Progression:
  CustomizerUnlockChapterMission: [1, 1]
  MorphemesUnlockChapterMission: [1, 1]
  BearCollectionUnlockChapterMission: [1, 1]
  GoogleSignInNag1: [1, 3]
  TVTimeUnlockChapterMission: [1, 4]
  TimeWarpsUnlockChapterMission: [1, 10]
  GoogleSignInNag2: [2, 3]
  SchoolUnlockChapterMission: [2, 4]
  DailyEventsUnlockChapterMission: [2, 12]
  GoogleSignInNag3: [3, 2]
  ReviewRequest: [3, 5]
  SecondReviewRequest: [4, 14]
  HardDifficultyUnlockChapterMission: [6, 12]
  MasterDifficultyUnlockChapterMission: [10, 11]
  SpecialEventsUnlockChapterMission: [2, 20]

Quizzes:
  Bonuses: [12, 9, 7, 5]
  MinimumQuizTiles: 2
  ChanceOfExtraQuizTiles: 0.025 # Percent.  e.g. 2% = 0.02
  BoardSizeForExtraQuizTiles: 48

Morphemes:
  # Morpheme tile count rules
  MorphemeScoreBonus: 15
  MinimumMorphemeTiles: 2
  BoardSizeForFewerMorphemeTiles: 48 # Smaller than this is just 1 tile
  BoardSizeForExtraMorphemeTiles: 62 # Bigger than this is 3 tiles

  # Spawning rules
  MinTileCountForMorphemes: 8
  AllowSequentialMorphemeTiles: false

  # The rest of the morpheme settings are currently disabled:
  ChanceOfExtraMorphemeTiles: 0.025 # Percent.  e.g. 2% = 0.02

